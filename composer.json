{
  "name": "silinternational/simplesamlphp-module-silauth",
  "description": "SimpleSAMLphp auth module implementing various security measures before calls to IdP ID Broker backend",
  "type": "simplesamlphp-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Phillip Shipley",
      "email": "phillip.shipley@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "ramsey/uuid": "^3.5",
    "rlanvin/php-ip": "^1.0",
    "roave/security-advisories": "dev-master",
    "silinternational/idp-id-broker-php-client": "^3.0",
    "silinternational/php-env": "^2.0",
    "silinternational/psr3-adapters": "^1.1 || ^2.0",
    "silinternational/yii2-json-log-targets": "1.0.0",
    "simplesamlphp/simplesamlphp": "~1.18.4",
    "yiisoft/yii2": "~2.0.12",
    "yiisoft/yii2-gii": "^2.0",
    "adldap2/adldap2": "^6.1",
    "fillup/fake-bower-assets": "^2.0",
    "google/recaptcha": "^1.1",
    "psr/log": "^1.0",
    "monolog/monolog": "^1.22"
  },
  "require-dev": {
    "behat/behat": "^3.2",
    "phpunit/phpunit": "^5.6.1"
  },
  "autoload": {
    "psr-4": {
      "Sil\\SilAuth\\": "src/",
      "Sil\\SilAuth\\features\\": "features/"
    }
  }
}
