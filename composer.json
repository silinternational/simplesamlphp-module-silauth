{
  "name": "silinternational/simplesamlphp-module-silauth",
  "description": "SimpleSAMLphp auth module implementing various security measures before calls to IdP ID Broker backend",
  "type": "simplesamlphp-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Phillip Shipley",
      "email": "phillip.shipley@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "codemix/yii2-streamlog": "^1.3",
    "ramsey/uuid": "^3.5",
    "rlanvin/php-ip": "^1.0",
    "silinternational/idp-id-broker-php-client": "^4.0.0",
    "silinternational/php-env": "^2.0",
    "silinternational/psr3-adapters": "^2.3 || ^3.0",
    "silinternational/yii2-json-log-targets": "^2.0",
    "simplesamlphp/simplesamlphp": "~1.18.6",
    "yiisoft/yii2": "~2.0.12",
    "yiisoft/yii2-gii": "^2.0",
    "fillup/fake-bower-assets": "^2.0",
    "google/recaptcha": "^1.1",
    "psr/log": "^1.0",
    "monolog/monolog": "^1.22"
  },
  "require-dev": {
    "behat/behat": "^3.2",
    "guzzlehttp/guzzle": "^6.0",
    "phpunit/phpunit": "^5.6.1",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Sil\\SilAuth\\": "src/",
      "Sil\\SilAuth\\features\\": "features/"
    }
  },
  "config": {
    "allow-plugins": {
      "simplesamlphp/composer-module-installer": true,
      "yiisoft/yii2-composer": true
    }
  }
}
